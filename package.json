{
  "name": "boilerplate-cra-ts-jest",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^27.5.2",
    "@types/morgan": "^1.9.9",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.9",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --testPathPattern=src/client/",
    "eject": "react-scripts eject",
    "server": "nodemon --exec ts-node --project tsconfig.server.json src/server/index.ts",
    "server:build": "tsc --project tsconfig.server.json",
    "server:start": "node dist/index.js",
    "server:test": "jest --config jest.server.config.js",
    "test:all": "npm run test && npm run server:test",
    "dev": "concurrently \"npm run server\" \"npm run start\"",
    "build:all": "npm run build && npm run server:build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },


  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/supertest": "^6.0.3",
    "concurrently": "^9.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0"
  }
}
